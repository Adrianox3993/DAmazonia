<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Apresentacao2">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="352dp"
        android:layout_height="36dp"
        android:fontFamily="@font/open_sans"
        android:text="Sua saúde merece atenção"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="432dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="381dp"
        android:layout_height="96dp"
        android:fontFamily="@font/open_sans"
        android:text="Por isso a D’Amazônia Produtos naturais fornece um catálogo digital para você que é nosso cliente de pertinho ou de outro Local do Brazil"
        android:textAllCaps="false"
        android:textColor="@color/colorAccent"
        android:textSize="18sp"
        android:typeface="serif"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="485dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="411dp"
        android:layout_height="403dp"
        android:scaleType="center"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="0dp"
        tools:srcCompat="@drawable/apresentacao2_" />
</androidx.constraintlayout.widget.ConstraintLayout>

